{
  "name": "money",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build:html": "mkdirp ./lib/docbase/ && cpx \"src/app/html/*\" ./lib/docbase/",
    "build:images": "mkdirp ./lib.docbase/images && cpx ./src/app/images/* ./lib/docbase/images/",
    "build:sass": "node-sass ./src/app/sass/main.scss ./lib/docbase/style.css",
    "build:watch:html": "npm run build:html -- --watch",
    "build:watch:images": "npm run build:images -- --watch",
    "build:watch:sass": "npm run build:sass -- --watch",
    "build:watch": "npm-run-all --parallel build:watch:*",
    "compile": "npm-run-all compile:typescript compile:es6",
    "compile:es6": "babel lib/es6 --out-dir lib/es5",
    "compile:typescript": "tsc --outDir lib/es6  --target ES6",
    "compile:watch": "npm-run-all --parallel compile:watch:*",
    "compile:watch:es6": "npm run compile:es6 -- --watch",
    "compile:watch:typescript": "npm run compile:typescript -- --watch",
    "compile:watch:webpack": "npm run compile:webpack -- --watch",
    "compile:webpack": "babel-node ./node_modules/.bin/webpack",
    "lint": "./node_modules/.bin/eslint '**/*.*s' --config .eslintrc",
    "start": "npm run compile && node lib/shell/shell.js",
    "start:server": "nodemon lib/es5/server/server.js",
    "start:database": "docker run --name docker-mongo -p 27017:27017 -d mongo",
    "test": "mocha --require ts-node/register src/test/test-setup.ts src/data/test/transaction-dao.it.ts"
  },
